void add1(char n[ ],float p,int quant)
{
        struct list1 *temp1=(struct list1 *)malloc(sizeof(struct list1));
        temp1->s_no=++ss;
        strcpy(temp1->name,n);
        temp1->quantity=quant;
        temp1->buy_price=p;
        temp1->current_price=p;
        temp1->buy_amount=product(temp1->quantity,temp1->buy_price);
        temp1->current_amount=product(temp1->current_price,temp1->quantity);
        temp1->percent_change=pchange(temp1->current_price,temp1->buy_price);
        temp1->price_change=prchange(temp1->current_price,temp1->buy_price);
        temp1->next=head1;
        head1=temp1;
    temp1=head1;
    printf("------------------------------------------------------------------------------------------------------------------------------------------\n");
    printf("                                                             STOCKS                                                                       \n");
    printf("------------------------------------------------------------------------------------------------------------------------------------------\n");
    printf("S.NO.|              NAME            | QUANTITY | BUY PRICE | CURRENT PRICE | BUY AMOUNT | CURRENT AMOUNT | PERCENT CHANGE | PRICE CHANGE | \n");
    while(temp1!=NULL)
    {
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        temp1=temp1->next;
    }

}
void buy()
{
    char names[25];
    char *nameadd;
    int n;

    struct database * tempd1=headd;
    printf("-----------------AVAILABLE STOCKS-----------------\n");
    printf("|           NAME                |  PRICE  |\n");
    while(tempd1->next!=NULL)
    {
    printf("|%-30s |%-9.2f|\n",tempd1->name,tempd1->price);
    tempd1=tempd1->next;
    }
    printf("ENTER THE NAME OF THE STOCK TO BE BOUGHT -> ");
    fflush(stdin);
    gets(names);
    printf("NO. OF SHARES-> ");
    fflush(stdin);
    scanf("%d",&n);
    tempd1=headd;
    int q;

    while(tempd1->next!=NULL)
    {

        if(strcmp(names,tempd1->name)==0)
        {
            printf("TO BUY ENTER 1, TO CANCEL ENTER 0->");
            fflush(stdin);
            scanf("%d",&q);
            if(q==1)
            {
                add1(names,tempd1->price,n);
            }
            else
            {
                return;
            }
        }
        tempd1=tempd1->next;
    }
    return;
}

void stocks()
{
    struct database *tempd1=headd;
    printf("-----------------------------TRENDING STOCKS-----------------------------\n");
    printf("|           NAME                |  PRICE  | PRICE CHANGE | \n");
    while (tempd1->next!=NULL)
    {
    printf("|%-31s|%-9.2f|%-14.2f|\n",tempd1->name,tempd1->price,tempd1->price_ch);
    tempd1=tempd1->next;
    }
    return;
}

void sell1()
{
    char namesell[25];
    int no;
    printf("ENTER THE NAME OF THE STOCK-> ");
    fflush(stdin);
    gets(namesell);
    printf("NO. OF SHARES-> ");
    fflush(stdin);
    scanf("%d",&no);
    int z=1;
    struct list1 *temp1 =head1;

    while (z=1)
    {

        while(temp1!=NULL)
        {

        if(strcmp(namesell,temp1->name)==0)
        {

        temp1->quantity=temp1->quantity-no;
        temp1->buy_amount=product(temp1->quantity,temp1->buy_price);
        temp1->current_amount=product(temp1->current_price,temp1->quantity);
        temp1->percent_change=pchange(temp1->current_price,temp1->buy_price);
        temp1->price_change=prchange(temp1->current_price,temp1->buy_price);
        temp1=head1;
        printf("------------------------------------------------------------------------------------------------------------------------------------------\n");
        printf("                                                             STOCKS                                                                       \n");
        printf("------------------------------------------------------------------------------------------------------------------------------------------\n");
        printf("S.NO.|              NAME            | QUANTITY | BUY PRICE | CURRENT PRICE | BUY AMOUNT | CURRENT AMOUNT | PERCENT CHANGE | PRICE CHANGE | \n");
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        temp1=temp1->next;
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        temp1=temp1->next;
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        temp1=temp1->next;
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        temp1=temp1->next;
        printf("%-5d|%-30s|%-10d|%-11.2f|%-15.2f|%-12.2f|%-16.2f|%-16.2f|%-14.2f|\n",temp1->s_no,temp1->name,temp1->quantity,temp1->buy_price,temp1->current_price,temp1->buy_amount,temp1->current_amount,temp1->percent_change,temp1->price_change);
        return ;
        }
        temp1=temp1->next;
    }


}
}
