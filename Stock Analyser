struct list1 * head1=NULL;
struct list2 * head2=NULL;
struct list3 * head3=NULL;
struct list4 * head4=NULL;
struct list5 * head5=NULL;

float product(float x,float y)
{
    float prod;
    prod=x*y;
    return prod;
}

float pchange(float q,float w)
{
    float per;
    per=((q-w)/w)*100;
    return per;

}
float prchange(float e,float r)
{
    float ch;
    ch=e-r;
    return ch;
}

